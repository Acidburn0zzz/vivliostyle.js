{
  "name": "vivliostyle-js-viewer",
  "version": "0.1.1",
  "description": "Paged viewer using Vivliostyle.js as its layout engine",
  "scripts": {
    "postinstall": "npm run build-vivliostyle",
    "build-vivliostyle": "cd node_modules/vivliostyle && npm install && npm run build",
    "setup-debug": "node scripts/setup-debug-vivliostyle.js",
    "build": "gulp build",
    "build-dev": "gulp build-dev",
    "serve": "gulp serve",
    "serve-dev": "gulp serve-dev",
    "test-local": "gulp test-local",
    "test-sauce": "gulp test-sauce"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vivliostyle/vivliostyle-js-viewer.git"
  },
  "author": "Vivliostyle Inc.",
  "bugs": {
    "url": "https://github.com/vivliostyle/vivliostyle-js-viewer/issues"
  },
  "homepage": "https://github.com/vivliostyle/vivliostyle-js-viewer",
  "dependencies": {
    "font-awesome": "^4.3.0",
    "vivliostyle": "vivliostyle/vivliostyle.js#master"
  },
  "devDependencies": {
    "babelify": "^6.1.3",
    "browser-sync": "^2.8.0",
    "browserify": "^11.0.1",
    "gulp": "^3.9.0",
    "gulp-changed": "^1.2.1",
    "gulp-compass": "^2.1.0",
    "gulp-ejs": "^1.1.1",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-util": "^3.0.6",
    "jasmine-core": "^2.3.4",
    "karma": "^0.13.9",
    "karma-browserify": "^4.3.0",
    "karma-jasmine": "^0.3.6",
    "karma-sauce-launcher": "^0.2.14",
    "karma-verbose-reporter": "0.0.3",
    "knockout": "^3.3.0",
    "mathjax": "^2.5.1",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.3.1"
  }
}
