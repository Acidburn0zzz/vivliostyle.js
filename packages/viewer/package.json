{
  "name": "@vivliostyle/viewer",
  "description": "Vivliostyle Viewer UI component, for rich paged viewing and printing in browser",
  "version": "2019.11.100-pre",
  "scripts": {
    "build": "gulp build && NODE_ENV=production webpack",
    "build-dev": "gulp build-dev && NODE_ENV=development webpack",
    "clean": "shx rm -rf lib .cache",
    "dev": "run-p dev:*",
    "dev:serve": "gulp serve-dev",
    "dev:webpack": "NODE_ENV=development webpack -w",
    "format": "prettier --write \"{*.{js,md,json},{src,test}/**/*.{ts,js}}\"",
    "lint": "run-p lint:*",
    "lint:scripts": "eslint src/ts --ext .ts -f codeframe",
    "lint:styles": "stylelint src/scss/{*.*,**/*.*}",
    "serve": "gulp serve",
    "setup-local-vivliostyle": "node scripts/setup-local-vivliostyle.js",
    "test": "NODE_ENV=development gulp test-local",
    "test-sauce": "NODE_ENV=development gulp test-sauce"
  },
  "dependencies": {
    "@vivliostyle/core": "^2019.11.100-pre",
    "font-awesome": "^4.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "browser-sync": "^2.26.7",
    "circular-dependency-plugin": "^5.2.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "find-package-json": "^1.2.0",
    "gulp": "^4.0.2",
    "gulp-changed": "^4.0.0",
    "gulp-ejs": "^4.1.1",
    "gulp-notify": "^3.2.0",
    "gulp-plumber": "^1.2.1",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "jasmine-core": "^3.4.0",
    "karma": "^4.2.0",
    "karma-jasmine": "^2.0.1",
    "karma-sauce-launcher": "^2.0.2",
    "karma-typescript-preprocessor": "^0.4.0",
    "karma-verbose-reporter": "0.0.6",
    "karma-webpack": "^4.0.2",
    "knockout": "^3.5.0",
    "node-sass": "^4.12.0",
    "node-sass-package-importer": "^5.3.2",
    "stylelint": "^11.0.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-scss": "^3.11.0",
    "terser-webpack-plugin": "^2.1.2",
    "tinyify": "^2.5.1",
    "ts-loader": "^6.2.0",
    "typescript": "^3.6.3",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9"
  },
  "author": "Vivliostyle Foundation",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/vivliostyle/vivliostyle/issues"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://github.com/vivliostyle/vivliostyle/packages/viewer",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vivliostyle/vivliostyle.git"
  }
